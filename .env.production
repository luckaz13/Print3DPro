# Configurações de produção para GitHub Pages
NODE_ENV=production
VITE_APP_NAME=Print3DPro
VITE_APP_VERSION=1.0.0
VITE_BASE_URL=/Print3DPro/
VITE_API_URL=https://luckaz13.github.io/Print3DPro/api
VITE_SITE_URL=https://luckaz13.github.io/Print3DPro/

# Analytics e Monitoramento
VITE_GA_TRACKING_ID=
VITE_GTM_ID=
VITE_HOTJAR_ID=

# SEO e Meta Tags
VITE_SITE_NAME=Print3DPro
VITE_SITE_DESCRIPTION=Serviços profissionais de impressão 3D com alta qualidade e precisão
VITE_SITE_KEYWORDS=impressão 3D, prototipagem, peças personalizadas, impressão 3D São Paulo
VITE_SITE_AUTHOR=Print3DPro
VITE_SITE_LANGUAGE=pt-BR

# Configurações de Performance
VITE_ENABLE_PWA=true
VITE_ENABLE_SERVICE_WORKER=true
VITE_CACHE_VERSION=1.0.0

# Configurações de Build
VITE_BUILD_ANALYZE=false
VITE_BUILD_SOURCEMAP=false
VITE_BUILD_MINIFY=true

# Configurações de Segurança
VITE_CSP_ENABLED=true
VITE_HTTPS_ONLY=true